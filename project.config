[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/me2151
[access "refs/for/refs/*"]
	read = group user/me2151
	push = group user/me2151
	label-Code-Review = -2..+2 group user/me2151
	label-Verified = -1..+1 group user/me2151
	rebase = group user/me2151
	removeReviewer = group user/me2151
	submit = group user/me2151
[access "refs/heads/*"]
	read = group user/me2151
	create = group user/me2151
	push = +force group user/me2151
	pushMerge = group user/me2151
[access "refs/tags/*"]
	push = group user/me2151
	push = +force group user/me2151
